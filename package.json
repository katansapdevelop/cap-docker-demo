{
  "name": "cap-docker-demo",
  "version": "2.1.9",
  "description": "Our primer sample for getting started in a nutshell.",
  "repository": "",
  "files": [
    "app",
    "srv",
    "db"
  ],
  "engines": {
    "node": ">=24"
  },
  "devDependencies": {
    "cds-plugin-ui5": "^0.13.0"
  },
  "dependencies": {
    "@sap/cds": "^9.7.0",
    "@cap-js/sqlite": "^2.1.1",
    "@sap-cloud-sdk/connectivity": "^4",
    "@sap-cloud-sdk/http-client": "^4",
    "@sap-cloud-sdk/resilience": "^4"
  },
  "scripts": {
    "start": "cds serve",
    "watch": "cds watch"
  },
  "license": "Apache-2.0",
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "impl": "@cap-js/sqlite",
        "credentials": {
          "url": ":memory:"
        }
      },
      "IntegrationContent": {
        "kind": "odata-v2",
        "model": "srv/external/IntegrationContent",
        "credentials": {
          "destination": "IS_APIS"
        }
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/docker-demo"
  ]
}
